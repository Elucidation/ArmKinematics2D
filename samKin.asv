L = [5 3]; % link lengths from base to tip
theta = [0 0]; % angles in degrees of joints


c = cosd(cumsum(theta)); %c1 c12 c123...
s = sind(cumsum(theta)); %s1 s12 s123...


% Base--joint1-->elbow--joint2-->End_Effector

% Rotation & offse
T_10 = [c(1) -s(1) 0; ...
        s(1) c(1)  0; ...
        0    0     1];

% Rotation and offset of joint
    T_10